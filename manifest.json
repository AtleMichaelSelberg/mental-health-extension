{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "mental-health-extension",
  "description": "An extension to improve your mental health",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ]
}

